# YouTube Data API (Required)
YOUTUBE_API_KEY=your_youtube_api_key_here

# Optional Configuration
YOUTUBE_TRANSCRIPT_LANG=en

# OAuth Configuration (Required only for private playlists)
GOOGLE_OAUTH_CLIENT_ID=your_oauth_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_oauth_client_secret_here
OAUTH_REDIRECT_URI=http://localhost:3000/oauth2callback

# Optional: Custom OAuth port (defaults to 3000)
OAUTH_PORT=3000
